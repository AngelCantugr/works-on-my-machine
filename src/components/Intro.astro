---
import { assetBase } from '$/utils/path';
const base = import.meta.env.BASE_URL;
const assetUrl = assetBase(base);
---
<img src={`${assetUrl}/assets/yay.svg`} alt="Yay!" />

<style>
    img {
        @apply mx-auto w-2/3 mt-6
    }
    h1 {
      @apply w-full justify-center text-center text-3xl font-bold text-purple-600 py-10
    }
</style>
